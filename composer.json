{
    "name": "cloudwp/api-abstract",
    "description": "Abstract API Bindings for SDK development",
    "authors": [
        {
            "name": "Rob Wittman",
            "email": "rob@ihsdigital.com"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "guzzlehttp/psr7": "^1.3",
        "php-http/curl-client": "^1.6",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/client-common": "^1.4"
    },
    "autoload" : {
        "psr-4" : {
            "CWP\\ADS\\AbstractApi\\" : "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "php-http/mock-client": "^0.3.3",
        "mockery/mockery": "^0.9.6"
    },
    "autoload-dev" : {
        "psr-4" : {
            "CWP\\ADS\\AbstractApi\\Tests\\" : "tests"
        }
    }
}
